    <function name="createPojoPackageName">
        <!-- Function must be called with 0 arguments -->
        <definition>org.blog3.pojo</definition>
    </function>
    <function name="createPojoClassName">
        <!-- Function must be called with 1 argument:
        1. Name of model object -->
        <definition>${arg1}</definition>
    </function>
    <function name="doesAttributeHaveBusinessKey">
        <!-- Function must be called with 2 arguments:
        1. Name of model object
        2. Name of model attribute -->
        <definition>${fmsnippet.condition.does_attribute_have_business_key}</definition>
    </function>
    <function name="doesReferenceHaveBusinessKey">
